<h1>New Project</h1>

<% @classproject.errors.full_messages.each do |msg| %>

<p><%= msg %></p>

<% end %>

<!-- Creates a POST request to "/users" -->

<%= form_for @classproject do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %>
	
	<%= f.label :summary %>
	<%= f.text_field :summary %>
	
	<%= f.label :codel_language %>
	<%= f.text :codel_language %>
	
	<%= f.label :github %>
	<%= f.submit :github %>
<% end %>

